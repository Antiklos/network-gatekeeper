#include <stdio.h>

enum EVENT {
  SEND_PAYMENT = 0
};

main(int args, char *argv[])
{
int event;
sscanf(argv[1], "%d", &event);

enum EVENT message = event;

printf("Argument is %i\n",message);

switch (message) {
  case SEND_PAYMENT:
    printf("Sending payment of %s\n",argv[2]);
}

return 0;
}


